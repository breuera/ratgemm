git clone https://github.com/libxsmm/libxsmm.git
cd libxsmm
make BLAS=0 -j

cd ..

mkdir catch2
wget https://github.com/catchorg/Catch2/releases/download/v2.13.9/catch.hpp -O ./catch2/catch.hpp

make all
./build/test_all


